{% for hour in available %}
  <label
    for="time-{{ hour.time }}"
    class="{% if hour.available === false %} disabled {% endif %}"
  >

    {% if hour.available === true %}
      <input
        id="time-{{ hour.time }}"
        name="date"
        value="{{ hour.value }}"
        type="radio"
      />
    {% endif %}

    {{ hour.time }}

  </label>
{% endfor %}
